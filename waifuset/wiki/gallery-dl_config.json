{
    "extractor": {
        "danbooru": {
            "filename": "danbooru_{id}.{extension}",
            "image-filter": "extension not in ('webm', 'mp4', 'gif')",
            "archive-format": "{id}",
            "archive-prefix": "",
            "skip": true
        },
        "postprocessors": [
            {
                "name": "metadata",
                "mode": true,
                "whitelist": [
                    "danbooru",
                    "moebooru",
                    "sankaku"
                ]
            }
        ]
    },
    "downloader": {
        "timeout": 60.0
    }
}